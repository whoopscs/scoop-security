    "homepage": "https://github.com/projectdiscovery/naabu",
    "license": "MIT",
    "notes": [
        "Scoop bucket for Cybersecurity by s0nd9r(https://github.com/s0nd9r/scoop-security)."
    ],
    "architecture": {
        "32bit": {
            "url": "https://github.com/projectdiscovery/naabu/releases/download/v2.3.0/naabu_2.3.0_windows_386.zip",
            "hash": "0daa588241c7f695fbc1ee4273b04c52c976d142e6951523224b6ceb991e7010"
        },
        "64bit": {
            "url": "https://github.com/projectdiscovery/naabu/releases/download/v2.3.0/naabu_2.3.0_windows_amd64.zip",
            "hash": "c5bd94392adb788c179acdfffee3db5f71aadc6f9ad9e038e551067535f6fef8"
        }
    },
    "pre_install": [
        "$wsShell = New-Object -ComObject WScript.Shell",
        "$shortcut = $wsShell.CreateShortcut([System.IO.Path]::Combine($env:USERPROFILE, 'Desktop', 'naabu.lnk'))",
        "$shortcut.TargetPath = \"cmd.exe\"",
        "$shortcut.Arguments  = \"/K cd /d $dir\"",
        "$shortcut.IconLocation  = \"$bucketsdir\\security\\icon\\naabu.ico\"",
        "$shortcut.Save()"
    ],
    "pre_uninstall": [
        "$shortcutPath = [System.IO.Path]::Combine([System.Environment]::GetFolderPath(\"Desktop\"), \"naabu.lnk\")",
        "if (Test-Path $shortcutPath) { Remove-Item $shortcutPath -Force }"
    ],
    "bin": "naabu.exe",
    "checkver": "github",
    "autoupdate": {
        "architecture": {
            "32bit": {
                "url": "https://github.com/projectdiscovery/naabu/releases/download/v$version/naabu_$version_windows_386.zip"
            },
            "64bit": {
                "url": "https://github.com/projectdiscovery/naabu/releases/download/v$version/naabu_$version_windows_amd64.zip"
            }
        }
    }
}
